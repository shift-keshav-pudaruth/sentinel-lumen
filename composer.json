    "description": "Lumen 5.4 integration for the Cartalyst 2.0 package.",
    "keywords": [
        "php",
        "cartalyst",
        "lumen",
        "sentinel"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*||5.2.*||5.3.*||5.4.*",
        "cartalyst/sentinel": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "Cartalyst\\Sentinel\\Lumen\\": "src/" }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
